@model TrenerDodajUrediVM

   

    <form asp-action="UrediLicencu" enctype="multipart/form-data" class="form-group">

        <div class="form-group">
            <label>Licenca: </label>
            <select asp-for="LicencaId" asp-items="@Model.Licenca" class="form-control"></select>

        </div>


        <div class="form-group">
            <label>Datum polaganja licence: </label>
            <input asp-for="@Model.DatumPolaganja" type="date" class="form-control" />

        </div>
        <div class="form-group">
            <label>Datum isteka licence: </label>
            <input asp-for="@Model.DatumIsteka" type="date" class="form-control" />

        </div>
        <input asp-for="TrenerId" type="hidden" />

        <button type="submit" class="btn btn-success">Snimi</button>
    </form>