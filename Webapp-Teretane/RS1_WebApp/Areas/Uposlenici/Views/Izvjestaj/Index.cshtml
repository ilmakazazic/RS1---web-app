@model IzvjestajVM
@{
    ViewData["Title"] = "Prikaz";

}

    <form asp-action="Prikazi" method="post" class="form-group">

        <div class="form-group">
            <label>Odaberi teretanu: </label>
            <select asp-for="TeretanaId" asp-items="@Model.Teretane" class="form-control"></select>
        </div>

        <div class="form-group">
            <label>Odaberi tip članarine: </label>
            <select asp-for="TipClanarineId" asp-items="@Model.TipClanarine" class="form-control"></select>
        </div>

        <button type="submit" class="btn btn-success">Snimi</button>

    </form>